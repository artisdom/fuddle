"2.0 (created: 2009/03/28 23:48:07)
set guioptions=rbT
set showtabline=1
"set runtimepath=/home/shi/.vimperator

map <F2> :tabprevious<CR>
map <F3> :tabnext<CR>
map <F12> ?next<CR><CR>
map \s :source ~/.vimperatorrc<CR>
map \m :set guioptions!=m<CR>
map \t :set guioptions!=T<CR>

noremap <C-l> <A-d>
noremap <C-k> <A-d><Tab>
noremap <F9> <c-v><F9>

"source! ~/.vimperatorrc.local

" vim: set ft=vimperator:
